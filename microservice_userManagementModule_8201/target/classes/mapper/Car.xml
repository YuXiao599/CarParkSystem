<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arthur.dao.CarDao">
    <insert id="insert">
    INSERT INTO "carinformation"("carid","carnumber","carinformation","userid")  VALUES(SEQ_CAR_ID.nextVal,#{carnumber}, #{carinformation}, #{userid})
  </insert>
</mapper>